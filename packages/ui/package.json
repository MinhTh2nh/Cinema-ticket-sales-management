{
    "name": "@chiketto/ui",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
        "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@chiketto/assets": "*"
    },
    "devDependencies": {
        "@chiketto/eslint-config-custom": "*",
        "@chiketto/tailwind-config": "*",
        "@chiketto/tsconfig": "*",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "concurrently": "^7.2.2",
        "eslint": "^7.32.0",
        "react": "^17.0.2",
        "tailwindcss": "^3.1.8",
        "tsup": "^6.1.3",
        "typescript": "4.8.4"
    }
}
